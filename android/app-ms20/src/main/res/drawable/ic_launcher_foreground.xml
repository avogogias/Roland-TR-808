<?xml version="1.0" encoding="utf-8"?>
<!-- Korg MS-20 icon: black panel with yellow patch cables motif -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Panel body -->
    <rect android:x="14" android:y="20" android:width="80" android:height="68"
        android:rx="2" android:fillColor="#2A2A2A" />

    <!-- Yellow top stripe -->
    <rect android:x="14" android:y="20" android:width="80" android:height="8"
        android:fillColor="#FFD700" />

    <!-- MS-20 label -->
    <path android:fillColor="#FFD700"
        android:pathData="M20,23 L22,23 L22,26 L23,23 L25,23 L25,26 L26,23 L28,23 L26,28 L24,28 L23,26 L22,28 L20,28 Z" />

    <!-- Dual VCO section - two oscillator knobs -->
    <circle android:cx="30" android:cy="42" android:r="6" android:fillColor="#333333" />
    <circle android:cx="30" android:cy="42" android:r="2" android:fillColor="#FFD700" />

    <circle android:cx="50" android:cy="42" android:r="6" android:fillColor="#333333" />
    <circle android:cx="50" android:cy="42" android:r="2" android:fillColor="#FFD700" />

    <!-- HPF + LPF section -->
    <circle android:cx="70" android:cy="42" android:r="5" android:fillColor="#333333" />
    <circle android:cx="70" android:cy="42" android:r="2" android:fillColor="#FFFFFF" />

    <circle android:cx="84" android:cy="42" android:r="5" android:fillColor="#333333" />
    <circle android:cx="84" android:cy="42" android:r="2" android:fillColor="#FFFFFF" />

    <!-- Patch bay dots (simulate patch points) -->
    <circle android:cx="20" android:cy="60" android:r="3" android:fillColor="#FFD700" />
    <circle android:cx="30" android:cy="60" android:r="3" android:fillColor="#FFD700" />
    <circle android:cx="40" android:cy="60" android:r="3" android:fillColor="#888888" />
    <circle android:cx="50" android:cy="60" android:r="3" android:fillColor="#888888" />
    <circle android:cx="60" android:cy="60" android:r="3" android:fillColor="#FFD700" />
    <circle android:cx="70" android:cy="60" android:r="3" android:fillColor="#888888" />

    <!-- Patch cable lines -->
    <path android:strokeColor="#FFD700" android:strokeWidth="1.5"
        android:fillColor="@android:color/transparent"
        android:pathData="M20,60 Q35,50 40,60" />
    <path android:strokeColor="#FF6B35" android:strokeWidth="1.5"
        android:fillColor="@android:color/transparent"
        android:pathData="M50,60 Q65,50 70,60" />

    <!-- Keyboard -->
    <rect android:x="14" android:y="74" android:width="80" android:height="14"
        android:fillColor="#F5F5F5" />
    <rect android:x="20" android:y="74" android:width="2" android:height="8"
        android:fillColor="#1A1A1A" />
    <rect android:x="26" android:y="74" android:width="2" android:height="8"
        android:fillColor="#1A1A1A" />
    <rect android:x="35" android:y="74" android:width="2" android:height="8"
        android:fillColor="#1A1A1A" />
    <rect android:x="41" android:y="74" android:width="2" android:height="8"
        android:fillColor="#1A1A1A" />
    <rect android:x="47" android:y="74" android:width="2" android:height="8"
        android:fillColor="#1A1A1A" />
</vector>
